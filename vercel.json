{
  "version": 1,
  "name": "e-commerce",
  "builds": [
    { "src": "/client/package.json", "use": "@vercel/node" },
    { "src": "/server/package.json", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/client/dist/$1" },
    { "src": "/api/(.*)", "dest": "/server/index.js" }
  ]
}
